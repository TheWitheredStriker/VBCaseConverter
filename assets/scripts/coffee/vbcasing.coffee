document.querySelector("#ConvertForm").addEventListener 'submit', (e) ->
	e.preventDefault()
	e.stopImmediatePropagation()
	uppercase = document.querySelector("#Input").value
	lowercase = uppercase

	for char of contable
		lowercase = lowercase.replace(new RegExp("\\b" + char + "\\b", 'g'), contable[char])
	
	document.querySelector('#Output').innerHTML = lowercase
	
